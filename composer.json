{
	"name": "pixel/packer",
	"description": "Laravel project/package cli-tool",
	"keywords": [
		"framework",
		"laravel",
		"laravel zero",
		"console",
		"cli"
	],
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Ivan Suazo",
			"email": "joshepw@gmail.com"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"guzzlehttp/guzzle": "6.5|^7.0",
		"illuminate/database": "^8.0",
		"illuminate/http": "^8.0",
		"illuminate/validation": "^8.42",
		"laminas/laminas-text": "^2.8",
		"nunomaduro/laravel-console-menu": "^3.2",
		"padraic/phar-updater": "^1.0.6",
		"spatie/laravel-ray": "^1.18"
	},
	"require-dev": {
		"laravel-zero/framework": "^8.7",
		"fakerphp/faker": "^1.9.1",
		"mockery/mockery": "^1.4.2",
		"pestphp/pest": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"bin": [
		"builds/packer"
	]
}
